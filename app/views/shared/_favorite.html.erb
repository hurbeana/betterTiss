<% if current_user %>
  <% if favorite?(favobj) %>
    <%= link_to "Unfavorite", "#{path}?tiss_id=#{favobj.tiss_id}&type=unfavorite", method: :put, class: "cardLinks" %>
  <% else %>
    <%= link_to "Favorite", "#{path}?tiss_id=#{favobj.tiss_id}&type=favorite", method: :put, class: "cardLinks" %>
  <% end %>
<% end %>
